{
  "name": "ol-describe-map",
  "version": "0.0.1",
  "description": "Get a configurable textual description of an OpenLayers map, to e.g. enhance accessibility",
  "main": "dist/src/index.js",
  "scripts": {
    "clean": "rimraf ./dist ./coverage",
    "build": "tsc",
    "build-examples": "vite build --config vite.config.js",
    "build:all": "npm run build-examples && npm run build",
    "lint": "eslint -c .eslintrc.js --ext ts src/",
    "pretest": "npm run typecheck && npm run lint",
    "serve-examples": "vite ./examples",
    "test": "jest",
    "test:watch": "jest --watch",
    "typecheck": "tsc --noEmit --project tsconfig.json"
  },
  "author": "Marc Jansen <jansen@terrestris.de>",
  "license": "BSD-2-Clause",
  "devDependencies": {
    "@babel/core": "^7.22.15",
    "@babel/preset-env": "^7.22.15",
    "@babel/preset-typescript": "^7.22.15",
    "@terrestris/eslint-config-typescript": "^4.0.0",
    "@types/jest": "^29.5.4",
    "babel-jest": "^29.6.4",
    "eslint": "^8.48.0",
    "highlight.js": "^11.8.0",
    "jest": "^29.6.4",
    "jest-environment-jsdom": "^29.6.4",
    "rimraf": "^5.0.1",
    "ts-node": "^10.9.1",
    "typescript": "^5.2.2",
    "vite": "^4.4.5"
  },
  "peerDependencies": {
    "ol": "^8.0.0"
  }
}
